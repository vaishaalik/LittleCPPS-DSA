#include<stdio.h>
void input(int *a, int *b, int *c)
{
	printf("enter the three values:");
	scanf("%d%d%d", a, b, c);
}
void compute(int a, int b, int c, int *min)
{
	if(a<b && a<c)
	*min=a;
	else if(b<a && b<c)
	*min=b;
	else
	*min=c;
}
void output(int min)
{
	printf("%d", min);
}
int main()
{
int a, b, c, min;
input(&a,&b,&c);
compute(a,b,c, &min);
output(min);
}
