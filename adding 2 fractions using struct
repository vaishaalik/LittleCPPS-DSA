#include<stdio.h>
struct frac
{
  int n,d;
};
int
input (struct frac z)
{
  struct frac z;
  z *n == a;
  z *d == b;
    return z;
}
void
compute (struct frac add(struct frac x, struct frac y),int z,int x,int y,int n, int d)
{
  {
    struct frac z;
    z.n == ((x.d) * (y.n) + (y.d) * (x.n));
    z.d == ((x.d) * (y.d));
  }
}
void
output (struct frac add(struct frac x, struct frac y), int x, int y, int n, int d)
{
  struct frac add = (((x.d) * (y.n) + (y.d) * (x.n)), ((x.d) * (y.d)));
  printf ("%d", struct frac add);
}

int
main ()
{
  struct frac n, d;
  int a, b, x, y, z;
  input (a,b,z,n,d);
  compute (struct frac add,x,y,int z,x,y,n,d);
  output ((struct frac add, x,y), int x,y,n,d);
}
